{% extends '::base.html.twig' %}

{% block title 'Upload test' %}

{% block body %}
    {{ parent() }}

    <div class="row">
        <div class="col-lg-12">
            <div class="swag-container">
                <h2 class="swag-text text-center">UPLOAD IMAGES</h2>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
            <form enctype="multipart/form-data" class="dropzone" action="{{ app.request.uri }}" method="post" name="dropzone_form" id="dropzone_form">
                {{ form_widget(form) }}
            {{ form_end(form) }}
        </div>
    </div>

    <div style="margin-bottom: 20px;">

    </div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
{% endblock %}